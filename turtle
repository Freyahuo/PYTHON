Turtle



import turtle


def draw_square():
    window = turtle.Screen()
    window.bgcolor("red")

    brad = turtle.Turtle()
    brad.shape("turtle")
    brad.speed(2)

    brad.fo(100)
    brad.right(90)
    brad.fo(100)
    brad.right(90)
    brad.fo(100)
    brad.right(90)
    brad.fo(100)
    brad.right(90)

    window.exitonclick()
